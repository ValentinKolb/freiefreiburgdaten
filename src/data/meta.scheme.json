{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "places": {
      "type": "array",
      "items": [
        {
          "type": "object",
          "properties": {
            "name": {
              "type": "string"
            },
            "description": {
              "type": "object",
              "properties": {
                "shortDescription": {
                  "type": "string"
                },
                "description": {
                  "type": "string"
                },
                "source": {
                  "type": "string"
                }
              },
              "required": [
                "shortDescription",
                "description",
                "source"
              ]
            },
            "category": {
              "type": "array",
              "items": [
                {
                  "type": "string"
                },
                {
                  "type": "string"
                }
              ]
            },
            "location": {
              "type": "object",
              "properties": {
                "long": {
                  "type": "string"
                },
                "lat": {
                  "type": "string"
                }
              },
              "required": [
                "long",
                "lat"
              ]
