                    "startYear": {
                      "type": "integer"
                    },
                    "endYear": {
                      "type": "integer"
                    },
                    "dataSheet": {
                      "type": "string"
                    },
                    "separator": {
                      "type": "string"
                    },
                    "source": {
                      "type": "string"
                    },
                    "sourceLink": {
                      "type": "string"
                    },
                    "graph": {
                      "type": "object",
                      "properties": {
                        "type": {
                          "type": "string"
                        }
                      },
                      "required": [
                        "type"
                      ]
                    }
                  },
                  "required": [
                    "identifier",
                    "startYear",
                    "endYear",
                    "dataSheet",
                    "separator",
                    "source",
                    "sourceLink",
                    "graph"
                  ]
                }
              ]
            }
          },
          "required": [
            "name",
            "description",
            "category",
            "location",
            "data"
          ]
        }
      ]
    }
  },
  "required": [
